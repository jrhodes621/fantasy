%h1 Listing fantasy_csv_imports
%table.table
  %thead
    %tr
      %th
        Id
      %th
        Fantasy Site
      %th
        File Name
      %th
        Rosters
  %tbody
    - @fantasy_csv_imports.each do |fantasy_csv_import|
      %tr
        %td
          = fantasy_csv_import._id
        %td
          = fantasy_csv_import.fantasy_site
        %td
          = fantasy_csv_import.file_name
        %td
          = link_to 'Rosters', import_path(fantasy_csv_import)
%br/
