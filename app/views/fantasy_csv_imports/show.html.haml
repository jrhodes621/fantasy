%p#notice= notice
%h2 Rosters

- @fantasy_csv_import.rosters.in_groups_of(3, false) do |row_rosters|
  .row
    - row_rosters.each do |roster|
      .roster.col-md-4
        %table.table
          %thead
            %tr
              %th
                Pos
              %th
                Name
              %th
                Salary
              %th
                Projected
              %th
                Actual
          %tbody
            - roster.roster_players.each do |player|
              %tr
                %td
                  = player.position
                %td
                  = player.name
                %td
                  = player.salary
                %td
                  = player.projected_points
                %td
                  = player.actual_points
= link_to 'Edit', edit_import_path(@fantasy_csv_import)
|
\#{link_to 'Back', imports_path}
